{
  "name": "mm",
  "private": true,
  "version": "0.0.1",
  "description": "Libraries for the MagicMirror experimental branch.",
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "nohoist": []
  },
  "scripts": {
    "postinstall": "preconstruct dev && manypkg check",
    "build": "preconstruct build",
    "watch": "preconstruct watch",
    "test": "jest",
    "run:all": "manypkg run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joshwilsonvu/mm.git"
  },
  "keywords": [
    "magic mirror",
    "smart mirror",
    "mirror UI",
    "modular"
  ],
  "authors": [
    "Josh Wilson <joshua.wilson@vanderbilt.edu>"
  ],
  "contributors": [
    "https://github.com/joshwilsonvu/mm/graphs/contributors"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joshwilsonvu/mm/issues"
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@changesets/cli": "^2.5.2",
    "@manypkg/cli": "^0.10.0",
    "@mm/babel-plugin-transform-config": "*",
    "@mm/babel-plugin-transform-mm2": "*",
    "@mm/core": "*",
    "@mm/hooks": "*",
    "@mm/mm2": "*",
    "@preconstruct/cli": "^1.1.7",
    "babel-jest": "^25.1.0",
    "babel-plugin-tester": "^8.0.1",
    "jest": "^24.9.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "redux": "^4.0.5"
  },
  "preconstruct": {
    "packages": [
      "packages/*"
    ]
  }
}
