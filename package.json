{
  "name": "mm",
  "private": true,
  "version": "0.0.1",
  "description": "Libraries for the MagicMirror experimental branch.",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "postinstall": "yarn dev && manypkg check",
    "build": "preconstruct build",
    "dev": "preconstruct dev",
    "test": "jest",
    "lint": "eslint packages/*",
    "changeset": "changeset",
    "release": "yarn build && changeset publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joshwilsonvu/mm.git"
  },
  "authors": [
    "Josh Wilson <joshua.wilson@vanderbilt.edu>"
  ],
  "contributors": [
    "https://github.com/joshwilsonvu/mm/graphs/contributors"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joshwilsonvu/mm/issues"
  },
  "dependencies": {
    "@babel/core": "^7.8.6",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@changesets/cli": "^2.5.2",
    "@manypkg/cli": "^0.10.0",
    "@preconstruct/cli": "^1.1.7",
    "@typescript-eslint/eslint-plugin": "2.x",
    "@typescript-eslint/parser": "2.x",
    "babel-eslint": "10.x",
    "babel-jest": "^25.1.0",
    "babel-plugin-tester": "^8.0.1",
    "eslint": "6.x",
    "eslint-config-react-app": "^5.2.0",
    "eslint-plugin-flowtype": "3.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jest": "^23.8.1",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-monorepo": "^0.2.1",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "1.x",
    "jest": "^24.9.0"
  },
  "preconstruct": {
    "packages": [
      "packages/*"
    ]
  }
}
