{
  "name": "mm",
  "private": true,
  "version": "0.0.0",
  "description": "Libraries for the MagicMirror experimental branch.",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "yarn workspaces foreach --verbose --parallel --topological-dev run build",
    "test": "jest",
    "test:changed": "jest --onlyChanged",
    "lint": "eslint packages",
    "changeset": "changeset",
    "precommit": "yarn && yarn build && yarn lint && yarn test",
    "publish": "yarn version check && yarn workspaces foreach --exclude mm --verbose npm publish --tolerate-republish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joshwilsonvu/mm.git"
  },
  "authors": [
    "Josh Wilson <joshua.wilson@vanderbilt.edu>"
  ],
  "contributors": [
    "https://github.com/joshwilsonvu/mm/graphs/contributors"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joshwilsonvu/mm/issues"
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.9.0",
    "@changesets/cli": "^2.5.2",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "axios": "^0.19.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "eslint": "^6.8.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^2",
    "execa": "^4.0.1",
    "fs-extra": "^9.0.0",
    "husky": "^4.2.3",
    "jest": "^26.0.1",
    "jest-watch-suspend": "^1.1.2",
    "strip-ansi": "^6.0.0",
    "typescript": "^3.9.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn precommit"
    }
  }
}
