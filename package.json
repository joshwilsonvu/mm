{
  "name": "mm",
  "private": true,
  "version": "0.0.0",
  "description": "Libraries for the MagicMirror experimental branch.",
  "workspaces": [
    "packages/*",
    "MagicMirror"
  ],
  "scripts": {
    "postinstall": "manypkg check && yarn build",
    "build": "wsrun --serial --exclude magicmirror --exclude-missing build",
    "test": "jest",
    "test:changed": "jest --onlyChanged",
    "test:inspect": "node --inspect node_modules/.bin/jest --runInBand",
    "lint": "eslint packages/*",
    "changeset": "changeset",
    "precommit": "yarn && yarn lint && yarn test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joshwilsonvu/mm.git"
  },
  "authors": [
    "Josh Wilson <joshua.wilson@vanderbilt.edu>"
  ],
  "contributors": [
    "https://github.com/joshwilsonvu/mm/graphs/contributors"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joshwilsonvu/mm/issues"
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.9.0",
    "@changesets/cli": "^2.5.2",
    "@manypkg/cli": "^0.11.1",
    "axios": "^0.19.2",
    "babel-eslint": "10.x",
    "babel-jest": "^25.1.0",
    "eslint": "^6.8.0",
    "eslint-config-react-app": "^5.2.0",
    "eslint-plugin-flowtype": "3.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jest": "^23.8.1",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "1.x",
    "execa": "^4.0.0",
    "fs-extra": "^9.0.0",
    "husky": "^4.2.3",
    "jest": "^24.9.0",
    "jest-watch-suspend": "^1.1.2",
    "strip-ansi": "^6.0.0",
    "wsrun": "^5.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn precommit"
    }
  }
}
