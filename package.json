{
  "name": "mm",
  "private": true,
  "version": "0.0.1",
  "description": "Libraries for the MagicMirror experimental branch.",
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "nohoist": []
  },
  "scripts": {
    "build": "lerna exec --parallel --ignore=@mm/babel-plugin-* -- babel --root-mode=upward lib --out-dir dist --source-maps --delete-dir-on-start",
    "test": "jest",
    "prepare": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joshwilsonvu/mm.git"
  },
  "keywords": [
    "magic mirror",
    "smart mirror",
    "mirror UI",
    "modular"
  ],
  "authors": [
    "Josh Wilson"
  ],
  "contributors": [
    "https://github.com/joshwilsonvu/mm/graphs/contributors"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joshwilsonvu/mm/issues"
  },
  "dependencies": {
    "@mm/babel-plugin-transform-config": "file:packages/babel-plugin-transform-config",
    "@mm/babel-plugin-transform-mm2": "file:packages/babel-plugin-transform-mm2",
    "@mm/core": "file:packages/core",
    "@mm/hooks": "file:packages/hooks",
    "@mm/mm2": "file:packages/mm2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "babel-jest": "^25.1.0",
    "babel-plugin-tester": "^8.0.1",
    "jest": "^24.9.0",
    "lerna": "^3.20.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "redux": "^4.0.5"
  }
}
